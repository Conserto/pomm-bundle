{
    "require": {
        "symfony/monolog-bundle": "^3.0",
        "conserto/pomm-model-manager": "4.0.x-dev as 4.0",
        "conserto/pomm-cli": "4.0.x-dev as 4.0",
        "conserto/pomm-symfony-bridge": "5.0.x-dev as 5.0",
        "conserto/pomm-foundation": "4.0.x-dev as 4.0",
        "symfony/framework-bundle": "^7.0",
        "jdorn/sql-formatter": "~1.2",
        "symfony/contracts": "^3.0",
        "symfony/http-kernel": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/form": "^7.0"
    },
    "require-dev": {
        "masterminds/html5": "^2.8",
        "phpunit/phpunit": "^10.0",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "autoload": {
        "psr-0": { "AppBundle\\": "src/" },
        "psr-4": { "PommProject\\PommBundle\\": "../sources/lib/" },
        "classmap": [ "app/AppKernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
