{
    "require": {
        "symfony/symfony": "^6.0",
        "symfony/monolog-bundle": "^3.0",
        "conserto/pomm-model-manager": "^3.0",
        "conserto/pomm-cli": "^3.0",
        "conserto/pomm-symfony-bridge": "^4.0",
        "conserto/pomm-foundation": "^3.0",
        "symfony/framework-bundle": "^6.0",
        "jdorn/sql-formatter": "~1.2",
        "symfony/contracts": "^2.5",
        "symfony/http-kernel": "^6.2"
    },
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "masterminds/html5": "^2.8"
    },
    "autoload": {
        "psr-0": { "AppBundle\\": "src/" },
        "psr-4": { "PommProject\\PommBundle\\": "../sources/lib/" },
        "classmap": [ "app/AppKernel.php" ]
    },
    "autoload-dev": {
        "files": ["tests/Features/bootstrap/FeatureContext.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
