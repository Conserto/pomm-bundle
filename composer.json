  "require": {
    "php": ">=8.1",
    "jdorn/sql-formatter": "~1.2",
    "conserto/pomm-model-manager": "^3.0",
    "conserto/pomm-cli": "^3.0",
    "conserto/pomm-symfony-bridge":  "^4.0",
    "symfony/framework-bundle": "~5.0|~6.0",
    "twig/twig": "~3.0",
    "sensio/framework-extra-bundle": "~5.0|~6.0"
