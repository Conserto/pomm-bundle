{
  "name": "conserto/pomm-bundle",
  "type": "symfony-bundle",
  "description": "The Symfony2 bundle for Pomm2",
  "keywords": ["orm", "postgresql", "database", "symfony", "pomm"],
  "homepage": "http://www.pomm-project.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas JOSEPH",
      "email": "nicolas.joseph@homecomputing.fr"
    },
    {
      "name": "GrÃ©goire HUBERT",
      "email": "hubert.greg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "jdorn/sql-formatter": "~1.2",
    "conserto/pomm-model-manager":  "dev-upgrade_php_8_1",
    "conserto/pomm-cli":  "dev-upgrade_php_8_1",
    "conserto/pomm-symfony-bridge":  "dev-upgrade_php_8_1",
    "symfony/framework-bundle": "~6.0",
    "twig/twig": "3.x-dev",
    "sensio/framework-extra-bundle": "^6.2"
  },
  "require-dev": {
    "symfony/console": "~6.0",
    "squizlabs/php_codesniffer": "~3.7",
    "phpstan/phpstan": "1.7.x-dev"
  },
  "suggest": {
      "symfony/web-profiler-bundle": "Display queries log",
      "sensio/distribution-bundle": "To use the entity param converter"
  },
  "autoload": {
    "psr-4": {
      "PommProject\\PommBundle\\": "sources/lib/"
    }
  },
  "extra": {
      "branch-alias": {
          "dev-master": "2.0-dev"
      }
  },
  "minimum-stability": "dev"
}
